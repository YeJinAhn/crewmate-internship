/* product detail */
.content-wrap.product-detail-wrap .content-inner {
    padding: 34px 0px  40px 0px;
    overflow: hidden;
}
.product-detail-wrap .indicator-wrap{margin:0 40px;}

.product-info-wrap{ width:auto; max-width:100%; height:auto; padding:0 40px; }
.product-info-wrap.product-detail-view{height:479px; border-bottom:1px solid #f4f4f4;}
.product-info-wrap.product-order-wrap{overflow:hidden; padding:25px 50px; background-color:#eaeaea;}
.product-info-wrap.product-list-wrap{}
.product-info-wrap.product-tech{padding:0 40px;}
.product-info-wrap.table-wrap{}
.product-info-wrap + .product-info-wrap{margin-top:40px;}
.product-detail-view + .product-info-wrap{margin-top:0;}

.product-info-wrap .text-title{font-size:26px; color:#333333;}
.product-info-wrap .text-title + div{margin-top:20px;}
.product-info-wrap .text-title + table{margin-top:20px;}

/* ---------------------------------------------- */
/*
상품상세 상단 이미지 리스트
*/
/* ---------------------------------------------- */
.product-detail-view #zoomTarget {position: absolute; width:430px; margin-left: -215px; left: 50%; top: -17px; z-index: 10;}
.detail-image-wrap{position:absolute; z-index:1; right:0; bottom:66px; width:765px; height:auto; overflow:hidden;}
.detail-image-wrap .inner-scroll-wrap{width:9999px; height:auto; overflow:hidden; position:relative;}
.detail-image-list{display:block; width:9999px; height:auto; overflow:hidden;}
.detail-image-list .list{float:left; width:430px; height:430px; overflow:hidden; opacity: 0.4; filter: alpha(opacity=40); /* For IE8 and earlier */}
.detail-image-list .list img{max-width:100%; height: 100%;}
.detail-image-list .list + .list{margin-left:118px;}
.detail-image-list .list.active{opacity: 1; filter: alpha(opacity=100); /* For IE8 and earlier */}

/* 상단 이미지 목록 : 섬네일 이미지 리스트 */
.detail-thumb-wrap{position:absolute; z-index:1; left:50px; bottom:35px; width:202px; height:auto; font-size:0; letter-spacing:-5px;}
.detail-thumb-wrap .thumb-wrap-inner{}
.detail-thumb-wrap .thumb-wrap-inner a.btn {float:left; position: relative; margin-left: -1px; margin-top: -1px;}

/* prev next btn */
.product-detail-view .btn-wrap{position:absolute; z-index:1; left:50%; bottom:30px; width:170px; height:auto; margin-left:-75px; font-size:0px; text-align:center; letter-spacing:-5px;}
.product-detail-view .btn-wrap button + button{margin-left:24px;}
button.btn-text-prev, button.btn-text-next{background-repeat:no-repeat; background-color:transparent; border:0; outline:0; color:#000; font-size:11px; cursor:pointer; letter-spacing:0;}
button.btn-text-prev{padding:5px 20px 5px 20px; text-align:left; background-image:url(/resources/images/common/front_icon_arrow_prev.png); background-position:left center;}
button.btn-text-next{padding:5px 20px 5px 20px; text-align:right; background-image:url(/resources/images/common/front_icon_arrow_next.png); background-position:right center;}


.product-info-wrap.table-wrap th{line-height:1.6;}
.product-info-wrap.table-wrap td{line-height:1.6;}
