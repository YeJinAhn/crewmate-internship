/*
주문정보
*/
/* ---------------------------------------------- */
.product-order-wrap .order-info{float:left; width:50%; height:auto; font-size:12px;}
.product-order-wrap .order-info .brand-wrap{width:auto; max-width:100%; height:auto; padding-right:89px;}
.product-order-wrap .order-info .btn-wrap{text-align:left;}
.product-order-wrap .order-info .btn-wrap button{vertical-align:middle;}
.product-order-wrap .order-info .btn-wrap button.btn-term{line-height:1; padding-bottom:1px;}
.product-order-wrap .order-info .btn-wrap button + span{padding-left:8px; font-size:12px; color:#000; vertical-align:middle;}
.product-order-wrap .order-info .brand-name{display:block; font-size:12px; font-weight:700;}
.product-order-wrap .order-info .brand-name.nepa{color:#12604e;}
/* .product-order-wrap .order-info .brand-name.nepa {color: #4a9281;} */
