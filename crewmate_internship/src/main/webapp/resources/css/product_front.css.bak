.product-order-wrap .order-info .brand-name.isenberg {color: #f26d1c;}
.product-order-wrap .order-info .brand-name .no-deduction {display: inline-block; color: #fff; height: 10px; margin-left: 8px; padding: 2px 3px; background: #b1b1b1; border: 1px solid #a1a1a1; font-size: 11px; line-height: 10px;}
.product-order-wrap .order-info .item-name{display:block; margin:15px 0 0 0; font-size:25px; color:#272727;}
.product-order-wrap .order-info .item-descript{margin-top:20px; font-size:12px; color:#8c8c8c; line-height:1.4;}
/* */
.product-order-wrap .order-info .cost-wrap{}
.product-order-wrap .order-info .cost-wrap {margin-top: 17px;}
.product-order-wrap .order-info .cost-wrap .normal-cost {position: relative; color: #5b5b5b; padding: 0 50px 0 10px; font-size:18px; font-weight: 700; background: url(/resources/images/common/front_bg_normal_cost.png) repeat-x 0 center;}
.product-order-wrap .order-info .cost-wrap .normal-cost i {position: absolute; display: inline-block; width: 5px; height: 16px; background: url(/resources/images/common/front_bg_normal_cost_arr.png) no-repeat 0 6px; right: 0; top: 2px;}
.product-order-wrap .order-info .cost-wrap .cost {font-size: 30px; font-weight: 700; color:#010101;}
.product-order-wrap .order-info .cost-wrap .normal-cost + .cost {margin-left: 20px;}
.product-order-wrap .order-info .cost-wrap .cost + .discount-percent {margin-left: 5px;}

/* */
.table-order{width:100%; height:auto; table-layout:fixed; font-size:12px;}
.table-order tbody th{height:25px; padding:10px 0px 10px 10px; color:#646464; text-align:left;}
.table-order tbody td{padding:10px 0px 10px 0px;}
.table-order tbody td select,
.table-order tbody td button{vertical-align:middle;}
.table-order tbody .tr-size th, .table-order tbody .tr-has-gift th {vertical-align: top; padding-top: 19px;}
.table-order tbody .tr-has-gift .list-bullet {margin-top: 5px;}
.table-order tbody .tr-has-gift .list-bullet li {color: #000; font-size: 12px;}
.table-order tbody .tr-has-gift .gift-list {margin-top:7px;}
.table-order tbody .tr-has-gift .gift-list li{color:#000; font-size:12px;}
.table-order tbody .tr-has-gift .gift-list li + li{margin-top:4px;}
.table-order tbody .tr-has-gift .gift-list li img{vertical-align:middle; position:relative; top:-2px;}
.table-order tbody .tr-none-sale td div {padding: 15px; background: #f8f8f8; border: 1px solid #e6e6e6; text-align: center; font-size: 13px; font-weight: 700;}
.table-order tbody .tr-delivery-st{display:none;}
.table-order tbody .tr-delivery-st td {padding:0;}
.table-order tbody .tr-delivery-n td{padding:0;}
.table-order tbody .tr-delivery-st .btn-gray{width:445px;}
.table-order tbody .tr-delivery-n .btn-gray{width:280px;}
.table-order tbody .tr-delivery-st .btn-favorite, .table-order tbody .tr-delivery-n .btn-favorite{min-width: inherit; width: 50px; padding: 0;}
.table-order tbody .btn-liter-gray{color:#5f5f5f; font-size:11px; font-weight:600;}
.table-order .select-colorsize{width:322px; height:26px; padding:4px 10px;}
.table-order .btn-prod-size, .table-order .btn-soldout {width: 103px; height: 24px; line-height:1; padding-bottom:1px;}
.table-order tbody .td-color {position: relative;}

/* 상품 상세 주문 정보[상단] */ 
.order-list-wrap{width:auto; max-width:100%; height:auto; background-color:#fff; border-top:1px solid #d4d4d4; border-bottom:1px solid #adadad;}
.order-item-list{display:block; width:auto; max-width:100%; height:auto; padding:14px 15px;}
