.order-list-wrap .gift-item{width:auto; max-width:100%; height:auto;}
.order-list-wrap .gift-item-group{width:auto; max-width:100%; height:auto; overflow:hidden; font-size:0; letter-spacing:-5px;}
.order-list-wrap .gift-item-group > span{float:left; font-size:12px; letter-spacing:0; vertical-align:middle;  line-height:29px;}
.order-list-wrap .gift-item-group select{letter-spacing:0; font-size:12px;}
.order-list-wrap .gift-item-group .gift-item-name{width:42%; /* 198px; */}
.order-list-wrap .gift-item-group .gift-item-select{width:29%; /* width:134px; */}
