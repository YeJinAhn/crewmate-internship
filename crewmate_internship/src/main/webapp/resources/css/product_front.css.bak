.order-list-wrap .gift-item-group .gift-item-cost{width:26%; padding-right:3%; /* width:117px; */ text-align:right;}

.order-list-wrap div + .gift-item-group{margin-top:5px;}

.order-list-wrap.item-empty .order-item-list {display: none;}
